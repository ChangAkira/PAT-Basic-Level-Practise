//1011 A+B 和 C  这道题目何意味？？？签到题吗
// 
#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>

int main() {
	double a, b, c;
	int i = 0;
	scanf("%d", &i);
	for (int j = 0; j < i; j++) {
		scanf("%lf %lf %lf", &a, &b, &c);
		printf("Case #%d: ", j + 1);
		if (a + b > c) {
			printf("true\n");
		}
		else {
			printf("false\n");
		}
	}
	return 0;
}
